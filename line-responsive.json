{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Literacy rate from 1976 to 2020",
  "width": 800,
  "height": 300,
  "data": {
    "url": "https://raw.githubusercontent.com/SinYiLau-32613865/FIT3179_Assignment-2_32613865/main/world_literacy_rate.csv"
  },
  "transform": [{"filter": "datum.Literacy_Rate >= 65"}],
  "mark": {"type": "line", "point": {"filled": false, "fill": "white"}},
  "encoding": {
    "x": {"timeUnit": "year", "field": "Year", "type": "temporal"},
    "y": {
      "field": "Literacy_Rate",
      "type": "quantitative",
      "scale": {"domain": [65, 90]}
    },
    "tooltip": [
      {"timeUnit": "year", "field": "Year", "type": "temporal"},
      {
        "field": "Literacy_Rate",
        "type": "quantitative",
        "title": "Literacy Rate (in %)"
      }
    ]
  },
  "config": {}
}