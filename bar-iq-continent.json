{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 700,
  "height": 300,
  "title": "Average IQ of the continent",
  "data": {
    "url": "https://raw.githubusercontent.com/SinYiLau-32613865/FIT3179_Assignment-2_32613865/main/csvData.csv"
  },
  "encoding": {
    "x": {"field": "Continent", "type": "nominal"},
    "y": {"aggregate": "mean", "field": "IQ", "type": "quantitative"}
  },
  "layer": [
    {
      "name": "layer_0",
      "mark": "bar",
      "encoding": {
        "color": {
          "field": "Continent",
          "type": "nominal",
          "scale": {
            "domain": [
              "Africa",
              "Asia",
              "Europe",
              "North America",
              "Oceania",
              "South America"
            ],
            "range": [
              "#e41a1c",
              "#984ea3",
              "#ff7f00",
              "#a6cee3",
              "#377eb8",
              "#a65628"
            ]
          }
        }
      }
    },
    {
      "mark": {
        "type": "text",
        "baseline": "middle",
        "dx": 0,
        "dy": -10,
        "fontSize": 11.5,
        "fontStyle": "italic"
      },
      "encoding": {
        "text": {
          "aggregate": "mean",
          "field": "IQ",
          "type": "quantitative",
          "format": ".2f"
        },
        "color": {"value": "black"}
      }
    }
  ],
  "config": {}
}